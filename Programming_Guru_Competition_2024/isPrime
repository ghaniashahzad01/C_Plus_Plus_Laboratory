#include <iostream>
using namespace std;

bool isPrime(int n) {
    // Handle numbers less than 2
    if (n <= 1) return false;

    // Loop from 2 to n-1 to check for divisibility
    for (int i = 2; i < n; i++) {
        if (n % i == 0) {
            return false; // If n is divisible by i, it's not prime
        }
    }

    // If no divisors found, n is prime
    return true;
}

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    if (isPrime(n))
        cout << "True" << endl;
    else
        cout << "False" << endl;

    return 0;
}
